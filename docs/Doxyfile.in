OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/src/ @CMAKE_CURRENT_SOURCE_DIR@/docs
EXTRACT_ALL            = NO
RECURSIVE			   = YES
EXCLUDE_SYMBOLS        = #include* 
EXCLUDE				   = @CMAKE_CURRENT_SOURCE_DIR@/src/lib
FILE_PATTERNS          = *.h
INLINE_SOURCES         = NO
PROJECT_NAME           = "Theta"
OPTIMIZE_OUTPUT_FOR_C  = YES
GENERATE_TREEVIEW      = YES # required!
HTML_EXTRA_STYLESHEET  = @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen-awesome.css @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen-awesome-sidebar-only.css
SOURCE_BROWSER		   = NO
VERBATIM_HEADERS	   = NO
SOURCE_BROWSER		   = NO
VERBATIM_HEADERS	   = NO
HIDE_UNDOC_MEMBERS     = YES
HIDE_UNDOC_CLASSES     = YES
HIDE_FRIEND_COMPOUNDS  = YES
HIDE_IN_BODY_DOCS      = YES